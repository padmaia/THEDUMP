{
  "name": "thedump",
  "version": "0.1.0",
  "description": "music social network",
  "main": "src/server/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "webpack -p --env production",
    "build:server": "babel src --out-dir lib --ignore *.spec.*",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rm -rf dist",
    "clean:server": "rm -rf lib",
    "dev": "nodemon",
    "lint": "eslint --ext js,jsx src",
    "posttest": "npm run lint",
    "start": "NODE_ENV=production node lib/server",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "connect-ensure-login": "^0.1.1",
    "express": "^4.16.2",
    "express-session": "^1.15.5",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^20.0.4",
    "nodemon": "^1.12.5",
    "react-test-renderer": "^16.2.0",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/padmaia/THEDUMP.git"
  },
  "author": "Maia Teegarden",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/padmaia/THEDUMP/issues"
  },
  "homepage": "https://github.com/padmaia/THEDUMP#readme"
}
